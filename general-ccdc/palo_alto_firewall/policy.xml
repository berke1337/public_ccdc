<?xml version="1.0"?>"
<config version="7.0.0" urldb="paloaltonetworks">
  <devices>
    <entry name="localhost.localdomain">
      <vsys>
        <entry name="vsys1">
          <application/>
          <application-group/>

          <!-- Services -->
          <service>
            <entry name="service-tcp-22">
              <protocol>
                <tcp>
                  <port>22</port>
                </tcp>
              </protocol>
            </entry>
            <entry name="service-tcp-443">
              <protocol>
                <tcp>
                  <port>443</port>
                </tcp>
              </protocol>
            </entry>
          </service>

          <!-- Rules -->
          <rulebase>
            <security>
              <rules>
                <entry name="allow-ping-firewall">
                  <to>
                    <member>Firewall</member>
                  </to>
                  <from>
                    <member>Pod-public</member>
                  </from>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>172.20.1.1</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <service>
                    <member>any</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>
                    <member>accept</member>
                  </action>
                  <application>
                    <member>ping</member>
                  </application>
                </entry>
                <entry name="allow-icmp-pod">
                  <to>
                    <member>Pod-Private</member>
                  </to>
                  <from>
                    <member>Pod-Private</member>
                  </from>
                  <source>
                    <member>192.168.10.1</member>
                    <member>192.168.10.10</member>
                    <member>192.168.10.50</member>
                  </source>
                  <destination>
                    <member>192.168.10.1</member>
                    <member>192.168.10.10</member>
                    <member>192.168.10.50</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <service>
                    <member>any</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>
                    <member>accept</member>
                  </action>
                  <application>
                    <member>ping</member>
                  </application>
                </entry>
                <entry name="allow-ssh-pod">
                  <to>
                    <member>Pod-Private</member>
                  </to>
                  <from>
                    <member>Pod-Private</member>
                  </from>
                  <source>
                    <member>192.168.10.1</member>
                    <member>192.168.10.10</member>
                    <member>192.168.10.50</member>
                  </source>
                  <destination>
                    <member>192.168.10.1</member>
                    <member>192.168.10.10</member>
                    <member>192.168.10.50</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <service>
                    <member>service-tcp-22</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>
                    <member>accept</member>
                  </action>
                  <application>
                    <member>any</member>
                  </application>
                </entry>
                <entry name="allow-ping-internet-2-firewall">
                  <to>
                    <member>Firewall</member>
                  </to>
                  <from>
                    <member>Internet</member>
                  </from>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>172.20.1.1</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <service>
                    <member>any</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>
                    <member>accept</member>
                  </action>
                  <application>
                    <member>ping</member>
                  </application>
                </entry>
                <entry name="allow-https-firewall">
                  <to>
                    <member>Firewall</member>
                  </to>
                  <from>
                    <member>Pod-public</member>
                  </from>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>172.20.1.1</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <service>
                    <member>service-tcp-443</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>
                    <member>accept</member>
                  </action>
                  <application>
                    <member>any</member>
                  </application>
                </entry>
                <entry name="allow-any-pod-2-internet">
                  <to>
                    <member>Internet</member>
                  </to>
                  <from>
                    <member>Pod-Public</member>
                  </from>
                  <source>
                    <member>172.20.1.1</member>
                    <member>172.20.1.10</member>
                    <member>172.20.1.50</member>
                  </source>
                  <destination>
                    <member>any</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <service>
                    <member>any</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>
                    <member>accept</member>
                  </action>
                  <application>
                    <member>any</member>
                  </application>
                </entry>
                <entry name="allow-ssh-firewall">
                  <to>
                    <member>Firewall</member>
                  </to>
                  <from>
                    <member>Pod-public</member>
                  </from>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>172.20.1.1</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <service>
                    <member>service-tcp-22</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>
                    <member>accept</member>
                  </action>
                  <application>
                    <member>any</member>
                  </application>
                </entry>
              </rules>
            </security>
            <default-security-rules>
              <rules>
                <entry name="intrazone-default">
                  <action>
                    <member>deny</member>
                  </action>
                </entry>
              </rules>
            </default-security-rules>
          </rulebase>
        </entry>
      </vsys>
    </entry>
  </devices>
</config>
